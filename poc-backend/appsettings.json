{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "Realm": "poc-realm",
    "AuthServerUrl": "http://host-dev:8080/",
    "SslRequired": "none",
    "Resource": "poc-client",
    "PublicClient": true,
    "ConfidentialPort": 0,
    "VerifyTokenAudience": false,
    "credentials": {
      "secret": "<secret-keycloal-client>"
    }
  }
}
